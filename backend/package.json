{
  "name": "reedi-backend",
  "version": "1.0.0",
  "description": "Backend API for Reedi social media platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "migrate:images": "node scripts/migrate-images.js",
    "convert:progressive": "node scripts/convert-to-progressive.js",
    "convert:progressive:cleanup": "node scripts/convert-to-progressive.js --cleanup",
    "test:progressive": "node scripts/test-progressive.js",
    "docker:start": "npm run db:deploy && npm run start:dev",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:auth": "NODE_ENV=test jest auth.test",
    "test:seed": "tsx __tests__/seed/seed.ts",
    "test:seed:reset": "tsx __tests__/seed/seed.ts --reset",
    "test:seed:extended": "tsx __tests__/seed/seed.ts --extended",
    "test:teardown": "tsx __tests__/seed/teardown.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@prisma/client": "^5.6.0",
    "@types/amqplib": "^0.10.7",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "amqplib": "^0.10.8",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.10.1",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.19",
    "@types/supertest": "^6.0.3",
    "bcrypt": "^6.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.6.0",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "social-media",
    "api",
    "nodejs",
    "typescript",
    "prisma"
  ],
  "author": "Reedi Team",
  "license": "MIT"
}

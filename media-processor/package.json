{
  "name": "media-processor",
  "version": "1.0.0",
  "description": "Media processing service for Reedi - handles video and image processing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "cli": "ts-node src/cli.ts",
    "standalone-cli": "ts-node src/standaloneCli.ts",
    "test-rabbitmq": "node test-integration.js",
    "cleanup": "node cleanup-temp-files.js",
    "cleanup:dry-run": "node cleanup-temp-files.js --dry-run",
    "cleanup:force": "node cleanup-temp-files.js --force",
    "cleanup:all": "node cleanup-temp-files.js --force --max-age=0"
  },
  "keywords": [
    "media",
    "processing",
    "video",
    "image",
    "ffmpeg"
  ],
  "author": "Reedi Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/yauzl": "^2.10.3",
    "amqplib": "^0.10.3",
    "axios": "^1.5.0",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "file-type": "^21.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "joi": "^17.9.2",
    "mime-types": "^2.1.35",
    "redis": "^4.6.10",
    "sharp": "^0.32.5",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
